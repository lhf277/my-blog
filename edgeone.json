{
  "caches": [
      {
        "source": "/*",
        "cacheTtl": 86400
      }
    ],
    "headers": [
      {
        "source": "/*",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "Cache-Control",
            "value": "max-age=7200"
          }
        ]
      }
    ]

}